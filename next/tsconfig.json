{
  "compilerOptions": {
    "target": "es6",
    "downlevelIteration": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*"
      ]
      /*

            "@/assets/icons": ["./assets/icons/index.ts"],

            "@services*/
      /*": ["./services*/
      /*"],


      /*"],
            "@components*/
      /*": ["./components*/
      /*"],
            "@assets/images": ["./assets/images/index.ts"],
            "@assets/images*/
      /*": ["./assets/images*/
      /*"],
            "@culturehq/add-to-calendar": ["./customTypes/@culturehq/add-to-calendar"]*/
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "next-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "customTypes"
  ]
}
