{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Str√°nky",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "layout": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "listing",
        "sublisting",
        "full_content",
        "content_with_sidebar",
        "partners",
        "blog_posts",
        "book_news",
        "documents",
        "event",
        "eventsListing",
        "news",
        "newsListing"
      ]
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "listingImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "promoted": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    },
    "date_added": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date"
    },
    "pageCategory": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "seo.seo"
    },
    "sections": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "sections.faq",
        "sections.flat-text",
        "sections.site-usefullness",
        "sections.form",
        "sections.subpages",
        "sections.table",
        "sections.accordion",
        "sections.divider",
        "sections.columned-text",
        "sections.cta",
        "sections.flat-text-center",
        "sections.sub-listing",
        "sections.documents",
        "sections.external-links",
        "sections.video",
        "sections.gallery",
        "sections.map",
        "sections.rental",
        "sections.blog-posts-listing",
        "sections.documents-listing",
        "sections.events-listing",
        "sections.new-books-listing",
        "sections.news-listing",
        "sections.partners"
      ]
    },
    "branchesServicesTo": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::branch.branch",
      "inversedBy": "servicePages"
    }
  }
}
