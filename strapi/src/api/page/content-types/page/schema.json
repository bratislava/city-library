{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "layout": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "listing",
        "sublisting",
        "full_content",
        "content_with_sidebar",
        "partners",
        "blog_posts",
        "book_news",
        "documents",
        "event",
        "eventsListing",
        "premises",
        "news",
        "newsListing",
        "localitiesListing",
        "locality"
      ]
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "listingImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "promoted": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    },
    "date_added": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date"
    },
    "blogPosts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "parentPage"
    },
    "pageCategory": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "seo.seo"
    },
    "sections": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "sections.faq",
        "sections.flat-text",
        "sections.site-usefullness",
        "sections.form",
        "sections.subpages",
        "sections.table",
        "sections.accordion",
        "sections.event-details",
        "sections.divider",
        "sections.columned-text",
        "sections.cta",
        "sections.flat-text-center",
        "sections.sub-listing",
        "sections.locality-details",
        "sections.documents",
        "sections.external-links",
        "sections.video",
        "sections.gallery"
      ]
    }
  }
}
